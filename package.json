{
  "name": "node-autoit-koffi",
  "version": "1.0.7",
  "description": "This Node.js module provides support for all AutoIt functions, allowing users to automate Windows GUI tasks seamlessly.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "bun run build && node dist/index.js",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format --write",
    "check": "biome check --write",
    "prepublishOnly": "bun run build",
    "publish:patch": "npm version patch --no-git-tag-version && npm publish --access public"
  },
  "dependencies": {
    "iconv-lite": "^0.6.3",
    "koffi": "^2.8.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/node": "^20.11.19",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "autoit",
    "napi",
    "nodejs",
    "ffi",
    "koffi"
  ],
  "readmeFilename": "README.md",
  "homepage": "https://github.com/nullmastermind/node-autoit-koffi",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nullmastermind/node-autoit-koffi.git"
  },
  "author": {
    "name": "Van Dong",
    "email": "thenullmastermind@gmail.com",
    "url": "https://github.com/nullmastermind"
  }
}
